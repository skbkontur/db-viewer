{
  "name": "@skbkontur/db-viewer-ui",
  "version": "1.0.0",
  "description": "Database Viewer with custom configuration",
  "main": "./index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:skbkontur/db-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/skbkontur/db-viewer/issues"
  },
  "homepage": "https://github.com/skbkontur/db-viewer",
  "keywords": [
    "DbViewer",
    "Database",
    "Cassandra",
    "Cql"
  ],
  "author": "Den Mukhametianov, Pavel Vostretsov",
  "scripts": {
    "build": "run-s build:types build:clean build:tsc \"build:copy:*\" build:patch build:pack",
    "build:clean": "rimraf dist",
    "build:tsc": "tsc --project tsconfig.prod.json",
    "build:patch": "node ./patch-version",
    "build:copy:src": "copyfiles --up 1 \"src/**/*.js\" \"src/**/*.less\" dist",
    "build:copy:meta": "copyfiles --flat package.json \"../*.md\" ../LICENSE dist",
    "build:pack": "cd dist && npm pack",
    "build:types": "dotnet ts-gen --assembly ./../DbViewer.TestApi/bin/netcoreapp3.1/win-x64/SkbKontur.DbViewer.TestApi.dll --outputDir ./src/Domain/Api --nullabilityMode NullableReference",
    "start": "webpack-dev-server --hot --env.api=fake",
    "start:prod": "webpack-dev-server --hot --env.api=real",
    "storybook": "start-storybook -p 6006 -s .",
    "lint": "run-s \"lint:*\"",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint ./ --ext .js,.jsx,.ts,.tsx",
    "lint:stylelint": "stylelint \"./**/*.less\"",
    "test": "mocha **/*.test.ts"
  },
  "peerDependencies": {
    "@skbkontur/react-icons": ">=3 <=4",
    "@skbkontur/react-ui": ">=0.52.0 <2.0.0",
    "react": ">=15 <=16",
    "react-dom": ">=15 <=16",
    "react-router": ">=4 <=5",
    "react-router-dom": ">=4 <=5"
  },
  "dependencies": {
    "@skbkontur/react-stack-layout": "^1.0.3",
    "@skbkontur/react-ui-validations": "^1.1.4",
    "copy-to-clipboard": "^3.3.1",
    "decimal.js": "^10.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "qs": "^6.9.3",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@skbkontur/react-icons": "^4.3.0",
    "@skbkontur/react-ui": "^1.11.1",
    "@storybook/addon-actions": "^6.0.0-beta.32",
    "@storybook/addons": "^6.0.0-beta.32",
    "@storybook/react": "^6.0.0-beta.32",
    "@types/chai": "^4.2.11",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "@types/qs": "^6.9.1",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-router": "^5.1.7",
    "@types/react-router-dom": "^5.1.5",
    "@types/storybook-react-router": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "less": "^3.11.1",
    "less-loader": "^6.0.0",
    "mocha": "^7.1.2",
    "nerdbank-gitversioning": "^3.1.74",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.20",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "rimraf": "^3.0.2",
    "storybook-react-router": "^1.0.8",
    "style-loader": "^1.2.0",
    "stylelint": "^13.6.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-standard": "^20.0.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
